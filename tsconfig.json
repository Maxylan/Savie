{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"resolveJsonModule": true,
        "esModuleInterop": true,
		"isolatedModules": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"outDir": "./dist",
		"rootDir": "./src"
	},
	"include": ["src", "src/icons"],
	"exclude": ["node_modules"]
}
